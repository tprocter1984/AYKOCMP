const updateConsentState = require('updateConsentState');
const getCookieValues = require('getCookieValues');
const createQueue = require('createQueue');

let consentObj = {
    preferences: 'denied',
    marketing: 'denied',
    statistics: 'denied'
  };

if( getCookieValues('amcookie_allowed').toString() !== '' ) {
  
  let consentAllowed = getCookieValues("amcookie_allowed")[0];
  consentAllowed = consentAllowed.split(',');
  consentAllowed.forEach( (consent) => {
    
    if ( consent == 1 ) {
      consentObj.preferences = 'granted';
    }
    if ( consent == 2 ) {
      consentObj.marketing = 'granted';
    }
    if ( consent == 3 ) {
      consentObj.statistics = 'granted';
    }

  });

}

updateConsentState({
  'ad_storage': consentObj.marketing,
  'analytics_storage': consentObj.statistics,
  'functionality_storage': consentObj.preferences,
  'personalization_storage': consentObj.preferences,
  'security_storage': 'granted',
  'wait_for_update': 500
});

const dataLayerPush = createQueue('dataLayer');
dataLayerPush({'event': 'Consent Updated'});

data.gtmOnSuccess();


